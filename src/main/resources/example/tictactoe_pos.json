{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Tic tac toe position",
  "description": "define a position of a tic tac toe case",
  "type": "object",
  "properties": {
    "row": {
      "type": "integer",
      "minimum": 1,
      "maximum": 3
    },
    "column": {
      "type": "integer",
      "minimum": 1,
      "maximum": 3
    }
  },
  "required": ["row", "column"]
}